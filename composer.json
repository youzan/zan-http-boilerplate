{
    "name": "{{NAME}}",
    "repositories": [
        {
            "type"  : "git",
            "url"   : "https://github.com/youzan/zanphp.git"
        },
        {
            "packagist": false
        },
        {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    ],
    "require": {
        "zanphp/zan": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "{{NAMESPACE}}": "src",
            "{{NAMESPACE}}Tests\\": "test",
            "{{NAMESPACE}}Init\\": "init"
        },
        "classmap": []
    },
    "autoload-dev": {
        "classmap": [
        ]
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false
    }
}
